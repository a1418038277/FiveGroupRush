<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_marginLeft="@dimen/dp_5"
        android:id="@+id/theme"
        android:text="活动主题:全国线上钢琴比赛"
        android:textSize="@dimen/dp_20"
        android:textColor="@color/bleak"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView

        android:layout_marginLeft="@dimen/dp_5"
        android:id="@+id/desc"
        android:text="活动主题活动主题活动主题活动主题活动主题活动主题"
        android:textSize="@dimen/dp_10"
        android:textColor="@color/bleak"
        android:layout_marginTop="@dimen/dp_5"
        app:layout_constraintTop_toBottomOf="@id/theme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/time"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="活动时间"
        android:textSize="@dimen/dp_15"
        android:textColor="@color/bleak"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/deadline"
        android:layout_marginLeft="@dimen/dp_35"
        android:text="截止报名"
        android:textSize="@dimen/dp_15"
        android:textColor="@color/bleak"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toRightOf="@id/time"
        app:layout_constraintTop_toBottomOf="@id/desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/time_start"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="2020-12-31"
        android:textSize="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/time_end"
        android:layout_marginLeft="@dimen/dp_35"
        android:text="2020-1-1"
        android:textSize="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toRightOf="@id/time"
        app:layout_constraintTop_toBottomOf="@id/deadline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/registration"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="报名费"
        android:textSize="@dimen/dp_15"
        android:textColor="@color/bleak"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/time_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/fee"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="200元"
        android:textSize="@dimen/dp_15"
        android:textColor="@color/red"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/time_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <View
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/bleak"
        app:layout_constraintTop_toBottomOf="@id/fee"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/m_name"
        android:layout_marginTop="@dimen/dp_20"
        app:layout_constraintTop_toBottomOf="@id/fee"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40">
        <TextView
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/dp_5"
            android:id="@+id/name"
            android:textColor="@color/bleak"
            android:textSize="@dimen/dp_15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="姓名"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/name_"
            android:text="*"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/name"
            android:textColor="@color/red"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <EditText
            android:hint="请填写参赛姓名"
            android:id="@+id/name_ed"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/name_"
            app:layout_constraintRight_toLeftOf="@id/icon"
            android:background="@null"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:gravity="center_vertical"
            android:id="@+id/icon"
            android:background="@mipmap/aaa"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="@dimen/dp_5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/bleak"
        app:layout_constraintTop_toBottomOf="@id/m_name"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/m_age"
        android:layout_marginTop="@dimen/dp_20"
        app:layout_constraintTop_toBottomOf="@id/m_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40">
        <TextView
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/dp_5"
            android:id="@+id/age"
            android:textColor="@color/bleak"
            android:textSize="@dimen/dp_15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="年龄"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/age_"
            android:text="*"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/age"
            android:textColor="@color/red"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <EditText
            android:hint="请输入年龄"
            android:id="@+id/age_ed"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/age_"
            app:layout_constraintRight_toLeftOf="@id/icon2"
            android:background="@null"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:gravity="center_vertical"
            android:id="@+id/icon2"
            android:background="@mipmap/aaa"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="@dimen/dp_5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/bleak"
        app:layout_constraintTop_toBottomOf="@id/m_age"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/m_call"
        android:layout_marginTop="@dimen/dp_20"
        app:layout_constraintTop_toBottomOf="@id/m_age"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40">
        <TextView
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/dp_5"
            android:id="@+id/call"
            android:textColor="@color/bleak"
            android:textSize="@dimen/dp_15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="电话"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/call_"
            android:text="*"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/call"
            android:textColor="@color/red"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <EditText
            android:hint="请输入电话"
            android:id="@+id/call_ed"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/call_"
            app:layout_constraintRight_toLeftOf="@id/icon3"
            android:background="@null"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:gravity="center_vertical"
            android:id="@+id/icon3"
            android:background="@mipmap/aaa"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="@dimen/dp_5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/bleak"
        app:layout_constraintTop_toBottomOf="@id/m_call"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/m_song"
        android:layout_marginTop="@dimen/dp_20"
        app:layout_constraintTop_toBottomOf="@id/m_call"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40">
        <TextView
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/dp_5"
            android:id="@+id/song"
            android:textColor="@color/bleak"
            android:textSize="@dimen/dp_15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="参赛曲目"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/song_"
            android:text="*"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/song"
            android:textColor="@color/red"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <EditText
            android:hint="请输入参赛曲目"
            android:id="@+id/song_ed"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/song_"
            app:layout_constraintRight_toLeftOf="@id/icon4"
            android:background="@null"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:gravity="center_vertical"
            android:id="@+id/icon4"
            android:background="@mipmap/aaa"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="@dimen/dp_5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout

        android:layout_margin="@dimen/dp_5"
        android:orientation="horizontal"
        android:id="@+id/li"
        app:layout_constraintTop_toBottomOf="@id/m_song"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_80">
        <ImageView
            android:src="@drawable/aaaa"
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_80"/>
    </LinearLayout>
    <Button
        android:text="提交"
        android:background="@drawable/round"
        app:layout_constraintTop_toBottomOf="@id/li"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</androidx.constraintlayout.widget.ConstraintLayout>