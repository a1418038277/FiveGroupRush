<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.AboutUsActivity"
    android:background="@color/colorGray">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/about_us_toolbar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@color/colorWhite">
        <ImageView
            android:id="@+id/about_back"
            android:layout_width="@dimen/dp_12"
            android:layout_height="@dimen/dp_18"
            android:background="@mipmap/lt_jt"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textColor="@color/colorBlack"
            android:textSize="@dimen/dp_16"
            android:gravity="center_horizontal"
            android:layout_marginRight="@dimen/dp_10"/>
    </androidx.appcompat.widget.Toolbar>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/colorGray"
        android:layout_marginTop="@dimen/dp_2"
        app:layout_constraintTop_toBottomOf="@id/about_us_toolbar" />
    <androidx.cardview.widget.CardView
        android:id="@+id/card_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/about_us_toolbar"
        android:layout_marginTop="@dimen/dp_19"
        app:layout_constraintLeft_toLeftOf="parent"
        app:cardUseCompatPadding="true"
        app:cardElevation="@dimen/dp_8"
        android:padding="@dimen/dp_6"
        android:background="@color/colorWhite"
        app:cardCornerRadius="@dimen/dp_10"
        android:layout_marginHorizontal="@dimen/dp_16">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <include android:id="@+id/call_our" layout="@layout/layout_about_us"/>
            <include android:id="@+id/clean_data" layout="@layout/layout_about_us"/>
            <include android:id="@+id/feedback" layout="@layout/layout_about_us"/>
            <include android:id="@+id/out_login" layout="@layout/layout_about_us" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>