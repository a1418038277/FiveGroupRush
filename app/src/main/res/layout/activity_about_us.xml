<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.AboutUsActivity"
    android:background="#CCFDF3">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_about"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_200"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:cardUseCompatPadding="true"
        app:cardElevation="@dimen/dp_8"
        android:padding="@dimen/dp_6"
        android:layout_margin="@dimen/dp_16">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <include android:id="@+id/call_our" layout="@layout/layout_about_us"/>
            <include android:id="@+id/clean_data" layout="@layout/layout_about_us"/>
            <include android:id="@+id/feedback" layout="@layout/layout_about_us"/>
            <include android:id="@+id/out_login" layout="@layout/layout_about_us" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>